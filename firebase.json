{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app" // This should match the name of your exported function in your code
      }
    ]
  },
  "functions": {
    "source": "."
  }
}
